<?xml version="1.0" encoding="UTF-8"?>
<!-- Parameters of Galacticus model Alpha -->
<!-- 17-March-2014                        -->
<parameters>
  <formatVersion>2</formatVersion>
  <version>0.9.4</version>

  <!-- Component selection -->
  <componentBasic value="standard"/>
  <componentBlackHole value="null"/>
  <componentDarkMatterProfile value="null"/>
  <componentDisk value="verySimple"/>
  <componentHotHalo value="verySimple"/>
  <componentSatellite value="verySimple"/>
  <componentSpheroid value="null"/>
  <componentSpin value="null"/>

  <!-- Cosmological parameters and options -->
  <cosmologyParameters value="simple">
    <HubbleConstant value="69.5723630486537"/>
    <OmegaMatter value="0.283812448723631"/>
    <OmegaDarkEnergy value="0.716187551276369"/>
    <OmegaBaryon value="0.0457967023357349"/>
    <temperatureCMB value="2.72548"/>
  </cosmologyParameters>
  <cosmologyFunctions value="matterLambda"/>
  <!-- Power spectrum options -->
  <cosmologicalMassVariance value="filteredPower">
    <sigma_8 value="0.818975624707821"/>
  </cosmologicalMassVariance>
  <transferFunction value="eisensteinHu1999">
    <neutrinoNumberEffective value="3.04"/>
    <neutrinoMassSummed value="0.0"/>
  </transferFunction>
  <powerSpectrumPrimordial value="powerLaw">
    <index value="0.955314462667311"/>
    <wavenumberReference value="1.0"/>
    <running value="0.0"/>
  </powerSpectrumPrimordial>
  <!-- Structure formation options -->
  <linearGrowth value="collisionlessMatter"/>
  <haloMassFunction value="tinker2008Generic">
    <a value="-0.127240525851282"/>
    <b value="0.0654435853813416"/>
  </haloMassFunction>
  <criticalOverdensity value="kitayamaSuto1996"/>
  <virialDensityContrast value="kitayamaSuto1996"/>

  <!-- Merger tree building options -->
  <mergerTreeConstructor value="build"/>
  <mergerTreeBuilder value="cole2000">
    <accretionLimit value="0.1"/>
    <mergeProbability value="0.1"/>
  </mergerTreeBuilder>
  <mergerTreeBranchingProbability value="parkinsonColeHelly">
    <G0 value="+0.570"/>
    <gamma1 value="+0.380"/>
    <gamma2 value="-0.010"/>
    <accuracyFirstOrder value="+0.025"/>
  </mergerTreeBranchingProbability>
  <mergerTreeMassResolution value="scaled">
    <massResolutionMinimum value="1.0e8"/>
    <massResolutionFractional value="1.0e-1"/>
  </mergerTreeMassResolution>
  <mergerTreeBuildMasses value="sampledDistributionUniform">
    <treesPerDecade value="250"/>
    <massTreeMinimum value="2.0e08"/>
    <massTreeMaximum value="1.0e15"/>
  </mergerTreeBuildMasses>
  <mergerTreeBuildMassDistribution value="haloMassFunction">
    <abundanceMaximum value="1.0e-4"/>
    <abundanceMinimum value="1.0e-7"/>
  </mergerTreeBuildMassDistribution>

  <!-- Substructure hierarchy options -->
  <mergerTreeNodeMerger value="singleLevelHierarchy"/>

  <!-- Dark matter halo structure options -->
  <darkMatterProfileDMO value="isothermal"/>

  <!-- Halo accretion options -->
  <accretionHalo value="simple">
    <opticalDepthReionization value=" 0.0845240757566554"/>
    <velocitySuppressionReionization value="38.8165530377000000"/>
  </accretionHalo>

  <!-- Hot halo gas cooling model options -->
  <coolingRate value="simpleScaling">
    <exponentCutOff value="0.646736423258"/>
    <timescale value="13.1613654864108"/>
    <exponentRedshift value="-1.19351808068"/>
    <widthCutOff value="0.469296451798381"/>
    <massCutOff value="25175271578.01"/>
  </coolingRate>
  <coolingTime value="simple"/>
  <starveSatellites value="true"/>

  <!-- Galactic structure solver options -->
  <galacticStructureSolver value="fixed"/>

  <!-- Galactic disk dynamics options -->
  <galacticDynamicsBarInstability value="stable"/>

  <!-- Star formation rate options -->
  <starFormationRateDisks value="timescale">
    <starFormationTimescale value="lowerLimited">
      <timescaleMinimum value="0.000129182223339982"/>
      <starFormationTimescale value="haloScaling">
        <exponentRedshift value="0.316329039863"/>
        <exponentVelocityVirial value="-3.34818453376"/>
        <timescale value="0.680886564265287"/>
      </starFormationTimescale>
    </starFormationTimescale>
  </starFormationRateDisks>

  <!-- Stellar populations options -->
  <stellarPopulationProperties value="instantaneous"/>
  <stellarPopulationSelector value="fixed"/>

  <initialMassFunction value="chabrier2001"/>
  <stellarPopulation value="standard">
    <recycledFraction value="0.0"/>
    <metalYield value="0.0"/>
  </stellarPopulation>

  <!-- Galaxy merger options -->
  <virialOrbit value="fixed">
    <velocityTangential value="+0.75"/>
    <velocityRadial value="-0.90"/>
  </virialOrbit>
  <satelliteMergingTimescales value="jiang2008">
    <timescaleMultiplier value="0.114092219110171"/>
  </satelliteMergingTimescales>
  <mergerMassMovements value="verySimple"/>
  <mergerRemnantSize value="null"/>

  <!-- Halo scale radii and concentrations -->
  <darkMatterProfileScaleRadius value="concentrationLimiter">
    <concentrationMinimum value="  4.0"/>
    <concentrationMaximum value="100.0"/>
    <darkMatterProfileScaleRadius value="concentration"/>
  </darkMatterProfileScaleRadius>

  <!-- Node evolution and physics -->
  <nodeOperator value="multi">
    <!-- Halo concentrations -->
    <nodeOperator value="darkMatterProfileScaleInterpolate"/>
    <!-- Halo spins -->
    <nodeOperator value="haloAngularMomentumRandom">
      <factorReset value="2.0"/>
    </nodeOperator>
    <nodeOperator value="haloAngularMomentumInterpolate"/>
    <!-- Star formation -->
    <nodeOperator value="starFormationDisks"/>
    <nodeOperator value="starFormationSpheroids"/>
    <!--Stellar feedback outflows-->
    <nodeOperator value="stellarFeedbackDisks">
      <stellarFeedbackOutflows value="rateLimit">
        <timescaleOutflowFractionalMinimum value="0.0032937916041941"/>
        <stellarFeedbackOutflows value="haloScaling">
          <exponentVelocity value="-16.4687643864"/>
          <exponentRedshift value="-7.53946527906"/>
          <fraction value="4.61431940043366e-05"/>
        </stellarFeedbackOutflows>
      </stellarFeedbackOutflows>
    </nodeOperator>
    <nodeOperator value="stellarFeedbackSpheroids">
      <stellarFeedbackOutflows value="rateLimit">
        <timescaleOutflowFractionalMinimum value="0.001"/>
        <stellarFeedbackOutflows value="powerLaw">
          <velocityCharacteristic value="250.0"/>
          <exponent value="3.5"/>
        </stellarFeedbackOutflows>
      </stellarFeedbackOutflows>
    </nodeOperator>
  </nodeOperator>

  <!-- Numerical tolerances -->
  <mergerTreeNodeEvolver value="standard">
    <odeAlgorithm value="rungeKuttaSecondOrder"/>
    <odeToleranceAbsolute value="0.01"/>
    <odeToleranceRelative value="0.01"/>
  </mergerTreeNodeEvolver>

  <diskMassToleranceAbsolute value="1.0e-6"/>
  <mergerTreeEvolver value="standard">
    <timestepHostAbsolute value="1.0"/>
    <timestepHostRelative value="0.1"/>
  </mergerTreeEvolver>

  <!-- Output options -->
  <mergerTreeOutputter value="standard">
    <outputReferences value="false"/>
  </mergerTreeOutputter>

</parameters>
